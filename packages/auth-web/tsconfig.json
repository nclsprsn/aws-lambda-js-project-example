{
  "compilerOptions": {
    "noImplicitAny": true,
    "strictNullChecks": true,
    "target": "es5",
    "module": "es6",
    "lib": [
        "es5",
        "es2015.promise"
    ],
    "moduleResolution": "node",
    "strict": true,
    "sourceMap": false,
    "noUnusedLocals": false
  },
  "exclude": [
    "dist",
    "events",
    "node_modules"
  ]
}