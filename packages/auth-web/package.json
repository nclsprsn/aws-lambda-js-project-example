{
  "name": "auth-web",
  "version": "2.0.0",
  "description": "Authentication web microservice",
  "main": "index.js",
  "private": true,
  "scripts": {
    "precommit": "pretty-quick --staged",
    "build-dev": "webpack --config webpack.common.js",
    "build-prd": "webpack --config webpack.prd.js",
    "run": "lambda-local -p default  -h handler",
    "test": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' nyc mocha --require source-map-support/register --require ts-node/register src/**/*.spec.ts src/*.spec.ts",
    "watch": "webpack --config webpack.common.js --watch --progress"
  },
  "author": "Nicolas Pierson",
  "license": "Beerware",
  "dependencies": {
    "aws-sdk": "^2.222.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^0.0.34",
    "@types/chai": "^4.1.2",
    "@types/mocha": "^5.0.0",
    "@types/node": "^9.6.2",
    "chai": "^4.1.2",
    "husky": "^0.14.3",
    "lambda-local": "^1.4.7",
    "mocha": "^5.0.5",
    "nyc": "^11.6.0",
    "pretty-quick": "^1.4.1",
    "source-map-support": "^0.5.4",
    "ts-loader": "^4.1.0",
    "ts-node": "^5.0.1",
    "typescript": "^2.8.1",
    "webpack": "^4.5.0",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-cli": "^2.0.14",
    "webpack-merge": "^4.1.2"
  },
  "nyc": {
    "all": true,
    "exclude": [
      "**/*.d.ts",
      "src/*.spec.ts",
      "src/**/*.spec.ts"
    ],
    "extension": [
      ".ts"
    ],
    "include": [
      "src/*.ts",
      "src/**/*.ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text"
    ],
    "watermarks": {
      "lines": [
        80,
        95
      ],
      "functions": [
        80,
        95
      ],
      "branches": [
        80,
        95
      ],
      "statements": [
        80,
        95
      ]
    }
  }
}
